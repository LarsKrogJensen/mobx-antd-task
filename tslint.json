{
    "extends": "tslint:recommended",
    "rulesDirectory": [],
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "arrow-parens": false,
        "trailing-comma": false,
        "quotemark": false,
        "semicolon": false,
        "new-parens": true,
        "no-unused-expression": true,
        "no-var-requires": false,
        "no-arg": true,
        "no-empty-interface": false,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        }
    },
    "jsRules": {
        "max-line-length": {
            "options": [120]
        }
    }
}